<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background12"
    tools:context="com.pajhama.svnitnme.svnitandme.file_complain">

    <TextView
        android:id="@+id/HSTLWLCM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/NMEHSTL"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="49dp"
        android:layout_marginTop="33dp"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/HSTLNME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/HSTLWLCM"
        android:layout_marginStart="17dp"
        android:layout_marginTop="50dp"
        android:text="HOSTEL NAME"
        android:textAllCaps="false"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textColorHighlight="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/NMEHSTL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/HSTLNME"
        android:layout_alignBottom="@+id/HSTLNME"
        android:layout_alignEnd="@+id/DESCRIP"
        android:layout_alignStart="@+id/DESCRIP"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/darker_gray"
        android:textColorHint="@color/common_google_signin_btn_text_dark_default"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/CMPLFLD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/urnme"
        android:layout_alignStart="@+id/HSTLNME"
        android:layout_marginBottom="16dp"
        android:text="COMPLAIN FIELD"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/FIELDCMPL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/CMPLFLD"
        android:layout_alignEnd="@+id/NMEHSTL"
        android:layout_alignStart="@+id/NMEHSTL"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/darker_gray"
        android:textColorHint="@color/common_google_signin_btn_text_dark_default"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/DETAIL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/DESCRIP"
        android:layout_alignBottom="@+id/DESCRIP"
        android:layout_alignStart="@+id/textView25"
        android:text="DESCRIPTION"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/DESCRIP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/CMPLNO"
        android:layout_alignStart="@+id/SUBMIT"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:inputType="textLongMessage"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/darker_gray"
        android:textColorHint="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/FILENO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/SUBMIT"
        android:layout_alignStart="@+id/DETAIL"
        android:layout_marginBottom="23dp"
        android:text="COMPLAIN ID"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/CMPLNO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/FILENO"
        android:layout_alignBottom="@+id/FILENO"
        android:layout_alignEnd="@+id/DESCRIP"
        android:layout_toEndOf="@+id/CMPLFLD"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/darker_gray"
        android:textColorHint="@color/common_google_signin_btn_text_dark_default"
        android:textSize="18sp" />
    <Button
        android:id="@+id/SUBMIT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:backgroundTint="@color/common_google_signin_btn_text_dark_focused"
        android:text="SUBMIT"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/urrm"
        android:layout_alignStart="@+id/CMPLFLD"
        android:layout_marginBottom="32dp"
        android:text="YOUR NAME"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/urrm"
        android:layout_alignBottom="@+id/urrm"
        android:layout_alignStart="@+id/DETAIL"
        android:text="ROOM NO"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/urnme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView25"
        android:layout_alignBottom="@+id/textView25"
        android:layout_alignEnd="@+id/FIELDCMPL"
        android:layout_alignStart="@+id/urrm"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/darker_gray"
        android:textColorHint="@color/common_google_signin_btn_text_dark_default"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/urrm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/DESCRIP"
        android:layout_alignEnd="@+id/DESCRIP"
        android:layout_alignStart="@+id/DESCRIP"
        android:layout_marginBottom="17dp"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/darker_gray"
        android:textColorHint="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="DEVELOPED AND CREATED BY PaJhaMa"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <Button
        android:id="@+id/BACKFC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/FILENO"
        android:backgroundTint="@color/common_google_signin_btn_text_dark_focused"
        android:text="BACK"
        android:textColor="@android:color/white" />

    <Button
        android:id="@+id/RESET"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/SUBMIT"
        android:layout_alignBottom="@+id/SUBMIT"
        android:layout_alignStart="@+id/FILENO"
        android:backgroundTint="@color/common_google_signin_btn_text_dark_focused"
        android:text="RESET"
        android:textColor="@android:color/white" /> />

</RelativeLayout>
