<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background6"
    tools:context="com.pajhama.svnitnme.svnitandme.GJ_SEC">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Gajjar HMC Welcomes You"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp"
        android:textStyle="bold" />

        <ImageButton
            android:id="@+id/HS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView2"
            android:layout_alignTop="@+id/JHS"
            app:srcCompat="@mipmap/hs"
            tools:ignore="UnknownId"
            android:onClick="data1"/>

        <ImageButton
            android:id="@+id/JHS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/WELFARE"
            android:layout_below="@+id/imageView"
            android:layout_marginTop="33dp"
            app:srcCompat="@mipmap/jhs"
            tools:ignore="UnknownId" />

        <ImageButton
            android:id="@+id/MESS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView2"
            android:layout_alignTop="@+id/JHS"
            app:srcCompat="@mipmap/mess"
            tools:ignore="UnknownId" />

        <ImageButton
            android:id="@+id/SPORTS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/HS"
            android:layout_below="@+id/HS"
            android:layout_marginTop="47dp"
            app:srcCompat="@mipmap/sports"
            tools:ignore="UnknownId" />

        <ImageButton
            android:id="@+id/CULTURAL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/MESS"
            android:layout_alignTop="@+id/SPORTS"
            app:srcCompat="@mipmap/cultural"
            tools:ignore="UnknownId" />

    <ImageButton
        android:id="@+id/ENVIRONMENT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/WELFARE"
        android:layout_alignTop="@+id/CULTURAL"
        app:srcCompat="@mipmap/environment"
        tools:ignore="UnknownId" />

    <ImageButton
        android:id="@+id/WELFARE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/MAGAZINE"
        android:layout_centerHorizontal="true"
        app:srcCompat="@mipmap/welfare" />

    <ImageButton
        android:id="@+id/TECHNICAL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CULTURAL"
        android:layout_alignTop="@+id/WELFARE"
        app:srcCompat="@mipmap/technical"
        tools:ignore="UnknownId" />

    <ImageButton
        android:id="@+id/MAGAZINE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/SPORTS"
        android:layout_below="@+id/SPORTS"
        android:layout_marginTop="35dp"
        app:srcCompat="@mipmap/magzine"
        tools:ignore="UnknownId" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        app:srcCompat="@mipmap/gajjar" />

    <TextView
        android:id="@+id/textview"
        style="@android:style/Widget.AutoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/BACKGJ"
        android:ems="16"
        android:inputType="textPersonName"
        android:text="DEVELOPED AND CREATED BY PaJhaMa"
        android:textAlignment="center"
        android:textColor="@android:color/darker_gray"
        android:textSize="14sp"
        android:textStyle="italic" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignEnd="@+id/textView12"
        android:layout_marginEnd="16dp"
        android:text="HS"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/JHS"
        android:layout_centerHorizontal="true"
        android:text="JHS"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/MESS"
        android:layout_below="@+id/MESS"
        android:layout_marginStart="13dp"
        android:text="MESS"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/SPORTS"
        android:layout_below="@+id/SPORTS"
        android:text="SPORTS"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ENVIRONMENT"
        android:layout_centerHorizontal="true"
        android:text="ENVIRONMENT"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CULTURAL"
        android:layout_below="@+id/CULTURAL"
        android:text="CULTURAL"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/MAGAZINE"
        android:layout_below="@+id/MAGAZINE"
        android:text="MAGAZINE"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/WELFARE"
        android:layout_below="@+id/WELFARE"
        android:text="WELFARE"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView16"
        android:layout_alignStart="@+id/TECHNICAL"
        android:text="TECHNICAL"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold" />

    <Button
        android:id="@+id/BACKGJ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView17"
        android:layout_alignParentBottom="true"
        android:text="BACK" />


</RelativeLayout>
